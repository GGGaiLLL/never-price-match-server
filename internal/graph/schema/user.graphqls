scalar Time

type User {
  id: ID!
  email: String!
  name: String!
  createdAt: Time!
  updatedAt: Time!
}

input CreateUserInput {
  email: String!
  name: String!
  password: String!
}

type Query {
  user(id: ID!): User
  checkEmailExist(email: String!): Boolean!
}

type Mutation {
  createUser(input: CreateUserInput!): User!
}
